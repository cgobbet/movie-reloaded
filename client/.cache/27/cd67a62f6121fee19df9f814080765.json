ine":98,"column":12},"source":"components/login-view/login-view.jsx","original":{"line":75,"column":12}},{"name":"string","generated":{"line":98,"column":31},"source":"components/login-view/login-view.jsx","original":{"line":75,"column":22}},{"generated":{"line":98,"column":37},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":22}},{"name":"onClick","generated":{"line":99,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":76,"column":2}},{"name":"onClick","generated":{"line":99,"column":2},"source":"components/login-view/login-view.jsx","original":{"line":76,"column":2}},{"generated":{"line":99,"column":9},"source":"components/login-view/login-view.jsx","original":{"line":76,"column":9}},{"name":"PropTypes","generated":{"line":99,"column":11},"source":"components/login-view/login-view.jsx","original":{"line":76,"column":11}},{"name":"func","generated":{"line":99,"column":30},"source":"components/login-view/login-view.jsx","original":{"line":76,"column":21}},{"generated":{"line":100,"column":0},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":22}},{"generated":{"line":100,"column":1},"source":"components/login-view/login-view.jsx","original":{"line":73,"column":0}}],"sources":{"components/login-view/login-view.jsx":"import React, { useState } from \"react\";\nimport PropTypes from \"prop-types\";\nimport { Link } from \"react-router-dom\";\nimport Container from \"react-bootstrap/Container\";\nimport Button from \"react-bootstrap/Button\";\nimport Form from \"react-bootstrap/Form\";\nimport Row from \"react-bootstrap/Row\";\n\n\nimport \"./login-view.scss\";\nimport axios from \"axios\";\n\nexport function LoginView(props) {\n  const [username, setUsername] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n\n  const handleSubmit = e => {\n    e.preventDefault();\n    /* Send a request to the server for authentication */\n    axios\n      .post(\"https://flix-app-test.herokuapp.com/login\", {\n        username: username,\n        password: password\n      })\n      .then(response => {\n        const data = response.data;\n        props.onLoggedIn(data); // triggers onLoggedIn method on “main-view.jsx”\n      })\n      .catch(e => {\n        console.log(\"no such user\");\n      });\n  };\n\n  return (\n    <Container>\n      <Row>\n        <div>\n          {/* <h1>Login</h1> */}\n          <Form className=\"form\">\n            <Form.Group controlId=\"formBasicUsername\">\n              <Form.Label>Username</Form.Label>\n              <Form.Control\n                type=\"text\"\n                value={username}\n                placeholder=\"Enter username\"\n                onChange={e => setUsername(e.target.value)}\n              />\n            </Form.Group>\n            <Form.Group controlId=\"formBasicPassword\">\n              <Form.Label>Password</Form.Label>\n              <Form.Control\n                type=\"password\"\n                value={password}\n                placeholder=\"Password\"\n                onChange={e => setPassword(e.target.value)}\n              />\n            </Form.Group>\n            <button className=\"btn-primary\" onClick={handleSubmit}>\n              Log in\n            </button>\n            <Link to={`/register`}>\n              <button className=\"btn-primary\">\n              Signup!</button>\n            </Link>\n          </Form>\n          <br />\n        </div>\n      </Row>\n    </Container>\n  );\n}\n\nLoginView.propTypes = {\n  username: PropTypes.string,\n  password: PropTypes.string,\n  onClick: PropTypes.func\n};\n"},"lineCount":null}},"error":null,"hash":"8512b11b4f14d92e321b281323384c4c","cacheData":{"env":{}}}